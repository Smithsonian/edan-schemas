{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"type": "object",
	"required": [
		"applicationId",
		"name",
		"secretKey"
	],
	"properties": {
		"publicSearch": {
			"type": "boolean"
		},
		"applicationId": {
			"type": "string"
		},
		"name": {
			"type": "string"
		},
		"unitCode": {
			"type": "string"
		},
		"status": {
			"type": "integer"
		},
		"secretKey": {
			"type": "string"
		},
		"description": {
			"type": "string"
		},
		"contact": {
			"type": "string"
		}
	}
}