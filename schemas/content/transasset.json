{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Annotation level metadata from transcription.si.edu",
    "type": "object",
    "required": [
        "assetImage",
        "transcriptData",
        "transcriptData",
        "projectOwner",
        "projectName",
        "assetCompleted",
        "assetStatus",
        "projectId"
    ],
    "additionalProperties": false,
    "properties": {
        "projectOwner": {
            "type": "string",
            "$id": "https://edan.si.edu/schema/content/properties/projectOwner"
        },
        "projectName": {
            "type": "string",
            "$id": "https://edan.si.edu/schema/content/properties/projectName"
        },
        "projectId": {
            "type": "string",
            "$id": "https://edan.si.edu/schema/content/properties/projectId"
        },
        "assetName": {
            "type": "string",
            "$id": "https://edan.si.edu/schema/content/properties/assetName"
        },
        "assetCompleted": {
            "type": "boolean",
            "$id": "https://edan.si.edu/schema/content/properties/assetCompleted"
        },
        "assetDisplay": {
            "type": "integer",
            "$id": "https://edan.si.edu/schema/content/properties/assetDisplay"
        },
        "assetStatus": {
            "type": "integer",
            "$id": "https://edan.si.edu/schema/content/properties/assetStatus"
        },
        "projectDisplay": {
            "type": "integer",
            "$id": "https://edan.si.edu/schema/content/properties/projectDisplay"
        },
        "assetImage": {
            "type": "string",
            "$id": "https://edan.si.edu/schema/content/properties/assetImage"
        },
        "transcriptData": {
            "type": "object",
            "$id": "https://edan.si.edu/schema/content/properties/transcriptData"
        },
        "metadata": {
            "type": "string",
            "$id": "https://edan.si.edu/schema/content/properties/metadata"
        }
