{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"type": "object",
	"required": [
		"listTitle",
		"settings",
		"feature",
		"title",
		"pageId",
		"url",
		"content"
	],
	"properties": {
		"title": {
			"type": "string",
			"$id": "http://jsonschema.net/title"
		},
		"url": {
			"type": "string",
			"$id": "http://jsonschema.net/url"
		},
		"listTitle": {
			"type": "string",
			"$id": "http://jsonschema.net/listTitle"
		},
		"content": {
			"type": "string",
			"$id": "http://jsonschema.net/content"
		},
		"feature": {
			"type": "object",
			"additionalProperties": false,
			"properties": {
				"thumbnail": {
					"type": "string",
					"$id": "http://jsonschema.net/feature/thumbnail"
				},
				"pages": {
					"type": "string",
					"$id": "http://jsonschema.net/feature/pages"
				},
				"alt": {
					"type": "string",
					"$id": "http://jsonschema.net/feature/alt"
				},
				"html": {
					"type": "string",
					"$id": "http://jsonschema.net/feature/html"
				},
				"type": {
					"type": "string",
					"$id": "http://jsonschema.net/feature/type"
				},
				"url": {
					"type": "string",
					"$id": "http://jsonschema.net/feature/url"
				},
				"media": {
					"type": "string",
					"$id": "http://jsonschema.net/feature/url"
				},
				"UxPageDisplay": {
					"type": "integer",
					"$id": "http://jsonschema.net/feature/UxPageDisplay"
				},
				"featurePageImage": {
					"type": "string",
					"$id": "http://jsonschema.net/feature/featurePageImage"
				},
				"featurePageImageAlt": {
					"type": "string",
					"$id": "http://jsonschema.net/feature/featurePageImageAlt"
				},
				"featurePageThumbnail": {
					"type": "string",
					"$id": "http://jsonschema.net/feature/featurePageThumbnail"
				},
				"teaserTitle": {
					"type": "string",
					"$id": "http://jsonschema.net/feature/teaserTitle"
				}
			},
			"$id": "http://jsonschema.net/feature"
		},
		"settings": {
			"type": "object",
			"additionalProperties": false,
			"properties": {
				"ux_objects_display": {
					"type": "integer",
					"$id": "http://jsonschema.net/settings/ux_objects_display"
				},
				"disableObjects": {
					"type": "integer",
					"$id": "http://jsonschema.net/settings/ux_objects_display"
				},
				"grid_count": {
					"type": "integer",
					"$id": "http://jsonschema.net/settings/grid_count"
				},
				"show_facets": {
					"type": "integer",
					"$id": "http://jsonschema.net/settings/show_facets"
				},
				"csc": {
					"type": "object",
					"properties": {
						"search_parameters": {
							"type": "object",
							"properties": {
								"date.slider": {
									"type": "string"
								}
							}
						}
					}
				}
			},
			"$id": "http://jsonschema.net/settings"
		},
		"pageId": {
			"type": "string",
			"$id": "http://jsonschema.net/pageId"
		},
		"objectGroupId": {
			"type": "string",
			"$id": "http://jsonschema.net/objectGroupId"
		},
		"published": {
			"type": "integer",
			"$id": "http://jsonschema.net/published"
		},
		"error": {
			"type": "string",
			"$id": "http://jsonschema.net/error"
		},
		"defaultPage": {
			"type": "object",
			"$id": "http://jsonschema.net/defaultPage"
		},
		"defaultPageId": {
			"type": "string",
			"$id": "http://jsonschema.net/defaultPageId"
		},
		"description": {
			"type": "string",
			"$id": "http://jsonschema.net/description"
		},
		"groupType": {
			"type": "integer",
			"$id": "http://jsonschema.net/groupType"
		},
		"featured": {
			"type": "integer",
			"$id": "http://jsonschema.net/featured"
		},
		"keywords": {
			"type": "string",
			"$id": "http://jsonschema.net/keywords"
		},
		"objects": {
			"type": "object",
			"additionalProperties": false,
			"properties": {
				"size": {
					"type": "integer",
					"$id": "http://jsonschema.net/objects/size"
				},
				"listName": {
					"type": "string",
					"$id": "http://jsonschema.net/objects/listName"
				},
				"listType": {
					"type": "integer",
					"$id": "http://jsonschema.net/objects/listType"
				},
				"items": {
					"type": "array",
					"$id": "http://jsonschema.net/objects/items",
					"items": {
						"type": "string",
						"$id": "http://jsonschema.net/objects/items"
					}
				}
			}
		},
		"menu": {
			"$id": "http://jsonschema.net/menu",
			"type": "array",
			"items": {
				"$id": "http://jsonschema.net/menu/items",
				"type": "object",
				"additionalProperties": false,
				"properties": {
					"listTitle": {
						"$id": "http://jsonschema.net/menu/items/listTitle",
						"type": "string"
					},
					"id": {
						"$id": "http://jsonschema.net/menu/items/id",
						"type": "string"
					},
					"title": {
						"$id": "http://jsonschema.net/menu/items/title",
						"type": "string"
					},
					"url": {
						"$id": "http://jsonschema.net/menu/items/url",
						"type": "string"
					}
				}
			}
		}
	},
	"additionalProperties": false,
	"$id": "http://jsonschema.net"
}